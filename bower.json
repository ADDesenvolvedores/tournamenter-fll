{
  "name": "tournamenter-obr",
  "description": "Tournamenter scorer app for OBR",
  "version": "1.0.0",
  "authors": [
    "Ivan Seidel <ivan@tendadigital.net>"
  ],
  "install": {
    "path": "public/tournamenter-obr/components",
    "sources": {
      "angular-bootstrap": {
        "mapping": [
          {
            "public/tournamenter-obr/.components/angular-bootstrap/ui-bootstrap.js": "ui-bootstrap.js"
          }
        ]
      },
      "font-awesome": {
        "mapping": [
          {
            "public/tournamenter-obr/.components/font-awesome/fonts/fontawesome-webfont.eot": "../fonts/fontawesome-webfont.eot"
          },
          {
            "public/tournamenter-obr/.components/font-awesome/fonts/fontawesome-webfont.svg": "../fonts/fontawesome-webfont.svg"
          },
          {
            "public/tournamenter-obr/.components/font-awesome/fonts/fontawesome-webfont.ttf": "../fonts/fontawesome-webfont.ttf"
          },
          {
            "public/tournamenter-obr/.components/font-awesome/fonts/fontawesome-webfont.woff": "../fonts/fontawesome-webfont.woff"
          }
        ]
      },
      "bootstrap": {
        "mapping": [
          {
            "public/tournamenter-obr/.components/bootstrap/fonts/glyphicons-halflings-regular.woff": "../fonts/glyphicons-halflings-regular.woff"
          },
          {
            "public/tournamenter-obr/.components/bootstrap/fonts/glyphicons-halflings-regular.ttf": "../fonts/glyphicons-halflings-regular.tff"
          }
        ]
      }
    }
  },
  "license": "MIT",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "jquery": "~2.1.1",
    "angular": "~1.2.0",
    "angular-resource": "~1.2.0",
    "angular-cookies": "~1.2.0",
    "angular-sanitize": "~1.2.0",
    "angular-route": "~1.2.0",
    "angular-animate": "~1.2.0",
    "font-awesome": "~4.1.0",
    "toastr": "~2.0.1",
    "underscore": "~1.5.2",
    "seiyria-bootstrap-slider": "~1.9.0",
    "bootstrap": "~3.2.0",
    "angular-bootstrap": "~0.10.0",
    "async": "^2.2.0"
  }
}
